type Query {
    characters: [Character]
    character(name: String): Character
    seasons: [Season]
    season(name: String): Season
}



type Character {
    name: String
    description: String
    appearsIn: [Episode]
}

type Episode {
    id: String
    name: String
    description: String
}

type Season {
    id: String
    name: String
    yearOfAiringStart: Int
    yearOfAiringEnd: Int
    episodes: [Episode]
}